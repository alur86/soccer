<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Game</th>
      <th>Player</th>
      <th>Strikes</th>
      <th>Yellow card</th>
      <th>Read card</th>
      <th>Penalty kiks</th>
      <th>Goal kiks</th>
      <th>Corner kicks</th>
      <th>Pushes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.team_id %></td>
        <td><%= event.game_id %></td>
        <td><%= event.player_id %></td>
        <td><%= event.strikes %></td>
        <td><%= event.yellow_card %></td>
        <td><%= event.read_card %></td>
        <td><%= event.penalty_kiks %></td>
        <td><%= event.goal_kiks %></td>
        <td><%= event.corner_kicks %></td>
        <td><%= event.pushes %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
