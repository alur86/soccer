<p id="notice"><%= notice %></p>

<h1>Second Team</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Game</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @second_teams.each do |second_team| %>
      <tr>
        <td><%= second_team.title %></td>
         <td><% game = Game.all.map { |g| [ g.id, g.game_date,] } %>
      <%= options_for_select(game) %> </td>
        <td><%= link_to 'Show', second_team %></td>
        <td><%= link_to 'Edit', edit_second_team_path(second_team) %></td>
        <td><%= link_to 'Destroy', second_team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Second Team', new_second_team_path %>
