<p id="notice"><%= notice %></p>

<h1>Two Team Players</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @two_team_players.each do |two_team_player| %>
      <tr>
        <td><%= two_team_player.first_name %></td>
        <td><%= two_team_player.last_name %></td>
        <td><%= two_team_player.age %></td>
        <td><%= link_to 'Show', two_team_player %></td>
        <td><%= link_to 'Edit', edit_two_team_player_path(two_team_player) %></td>
        <td><%= link_to 'Destroy', two_team_player, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Two Team Player', new_two_team_player_path %>
